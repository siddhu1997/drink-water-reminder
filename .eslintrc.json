{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "class-methods-use-this": "off",
        "import/no-unresolved": "off",
        "global-require": "off",
        "import/no-dynamic-require": "off",
        "no-console": "off"
    },
    "ignorePatterns": ["taskdef.json", "test/**"]
}
